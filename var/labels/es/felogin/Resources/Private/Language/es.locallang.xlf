  <file source-language="en" datatype="plaintext" original="EXT:felogin/Resources/Private/Language/locallang.xlf" date="2011-10-17T20:22:32Z" product-name="felogin" target-language="es">
    <header/>
    <body>
      <trans-unit id="welcome_header" resname="welcome_header" approved="yes">
        <source>User login</source>
        <target state="final">Iniciar sesión de usuario</target>
      </trans-unit>
      <trans-unit id="logout_header" resname="logout_header" approved="yes">
        <source>You have logged out.</source>
        <target state="final">Acaba de cerrar su sesión.</target>
      </trans-unit>
      <trans-unit id="error_header" resname="error_header" approved="yes">
        <source>Login failure</source>
        <target state="final">Falló el inicio de sesión</target>
      </trans-unit>
      <trans-unit id="success_header" resname="success_header" approved="yes">
        <source>Login successful</source>
        <target state="final">Inicio de sesión correcto</target>
      </trans-unit>
      <trans-unit id="success_message" resname="success_message" approved="yes">
        <source>You are now logged in as '###USER###'</source>
        <target state="final">Ha iniciado sesión como '###USER###'</target>
      </trans-unit>
      <trans-unit id="status_header" resname="status_header" approved="yes">
        <source>Current status</source>
        <target state="final">Estado actual</target>
      </trans-unit>
      <trans-unit id="cookie_warning" resname="cookie_warning" approved="yes">
        <source>Warning: There is a possibility that cookies are not enabled in your web browser! If your login disappears on your next click that is the case and you should enable cookies (or accept cookies from this website) immediately!</source>
        <target state="final">Advertencia: ¡Existe la posibilidad de que las cookies no estén habilitadas en su navegador! ¡Si su nombre de usuario desaparece en el siguiente clic, debe habilitar las cookies (o aceptar las cookies de este sitio web) inmediatamente!</target>
      </trans-unit>
      <trans-unit id="login" resname="login" approved="yes">
        <source>Login</source>
        <target state="final">Iniciar sesión</target>
      </trans-unit>
      <trans-unit id="logout" resname="logout" approved="yes">
        <source>Logout</source>
        <target state="final">Finalizar sesión</target>
      </trans-unit>
      <trans-unit id="send_password" resname="send_password" approved="yes">
        <source>Send password</source>
        <target state="final">Enviar contraseña</target>
      </trans-unit>
      <trans-unit id="reset_password" resname="reset_password" approved="yes">
        <source>Reset Password</source>
        <target state="final">Restablecer contraseña</target>
      </trans-unit>
      <trans-unit id="change_password_header" resname="change_password_header" approved="yes">
        <source>Change your password</source>
        <target state="final">Cambiar su contraseña</target>
      </trans-unit>
      <trans-unit id="change_password" resname="change_password" approved="yes">
        <source>Change your password</source>
        <target state="final">Cambiar su contraseña</target>
      </trans-unit>
      <trans-unit id="newpassword_label1" resname="newpassword_label1" approved="yes">
        <source>Enter new password</source>
        <target state="final">Introduzca su nueva contraseña</target>
      </trans-unit>
      <trans-unit id="newpassword_label2" resname="newpassword_label2" approved="yes">
        <source>Repeat new password</source>
        <target state="final">Repita la nueva contraseña</target>
      </trans-unit>
      <trans-unit id="forgot_header" resname="forgot_header" approved="yes">
        <source>Forgot your password?</source>
        <target state="final">¿Ha olvidado su contraseña?</target>
      </trans-unit>
      <trans-unit id="forgot_email_password" resname="forgot_email_password" xml:space="preserve" approved="yes">
				<source>Your password
Hi %s

Your username is "%s"
Your password is "%s"</source>
			<target state="final">Su contraseña
Hola %s

Su nombre de usuario es "%s"
Su contraseña es "%s"</target></trans-unit>
      <trans-unit id="forgot_message_emailSent" resname="forgot_message_emailSent" approved="yes">
        <source>Your password has now been sent to the email address %s</source>
        <target state="final">Su contraseña se ha enviado a la dirección de correo %s</target>
      </trans-unit>
      <trans-unit id="forgot_header_backToLogin" resname="forgot_header_backToLogin" approved="yes">
        <source>Return to login form</source>
        <target state="final">Volver al formulario de inicio de sesión</target>
      </trans-unit>
    </body>
